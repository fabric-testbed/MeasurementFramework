# Docker Exporter

  docker_exporter:
    container_name: "{{ install_name }}_docker_exporter"
    image: prometheusnet/docker_exporter
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock

    logging:
      driver: json-file
      options:
        max-size: "1024m" 
        max-file: "5"

    restart: always